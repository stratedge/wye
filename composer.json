{
    "name": "stratedge/wye",
    "description": "A library that makes unit testing database-driven code in PHP a breeze. Mock the native PDO class, define query results, and inspect executed statements.",
    "type": "library",
    "keywords": [
        "database",
        "mock",
        "mocking",
        "mysql",
        "pdo",
        "phpunit",
        "replacement",
        "test",
        "testing",
        "unit",
        "unit-test",
        "unit-testing",
        "unittest"
    ],
    "homepage": "https://github.com/stratedge/wye",
    "license": "MIT",
    "authors": [
        {
            "name": "Jarret Byrne",
            "email": "jarret@solvomedia.com"
        }
    ],
    "support": {
        "issues": "https://github.com/stratedge/wye/issues",
        "wiki": "https://github.com/stratedge/wye/wiki",
        "docs": "https://github.com/stratedge/wye/wiki"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "raveren/kint": "^1.0",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Stratedge\\Wye\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
